      AND FERC_DATA.parent_header = GAAP_DATA.header_id
      AND COALESCE(GAAP_DATA.source, FERC_DATA.source) NOT IN ('Payables', 'Receivables', 'Receipt Accounting', 'projects', 'Project Accounting')
